{
    "name": "mix-code/jawaly-sms",
    "description": "This is my package jawaly-sms",
    "keywords": [
        "mix-code",
        "jawaly-sms"
    ],
    "homepage": "https://github.com/mix-code/jawaly-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "MixCode",
            "email": "mixcode2025@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.15",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MixCode\\JawalySms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MixCode\\JawalySms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MixCode\\JawalySms\\JawalySmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
